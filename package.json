{
  "name": "deep-roots",
  "version": "1.0.0",
  "description": "MERN Stack web application for Deep Roots",
  "main": "server.js",
  "scripts": {
    "test": "mocha server/test",
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Team SixC at UF",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.0",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^3.1.2",
    "cors": "^2.8.5",
    "ejs": "^2.7.1",
    "express": "^4.17.1",
    "express-ejs-layouts": "^2.5.0",
    "express-session": "^1.17.0",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^6.2.2",
    "mongoose": "^5.7.5",
    "morgan": "^1.9.1",
    "nodemailer": "^6.3.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "should": "^13.2.3",
    "supertest": "^4.0.2",
    "surveyjs-react": "^1.0.1"
  },
  "devDependencies": {
    "nodemon": "^1.19.4"
  }
}
